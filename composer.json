{
    "name": "roelmagdaleno/wp-settings-page-fields",
    "description": "A package to render form fields in WordPress settings pages.",
    "version": "0.1.3",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Roel\\WP\\Settings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Roel\\WP\\Settings\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Roel Magdaleno",
            "email": "roelmagdaleno@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "yoast/phpunit-polyfills": "^1.0",
        "pestphp/pest": "^1.21",
        "phpmd/phpmd": "@stable",
        "phpro/grumphp": "^1.12",
        "squizlabs/php_codesniffer": "3.*",
        "wp-coding-standards/wpcs": "^2.2",
        "phpcompatibility/phpcompatibility-wp": "*",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    }
}
